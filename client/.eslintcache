dtracker/client/src/index.js",[],["32","33"],"/Users/user/Documents/Projects/fundtracker/client/src/App.js",["34"],"/Users/user/Documents/Projects/fundtracker/client/src/LineChart.js",["35"],"import React, {useEffect} from \"react\";\nimport * as d3 from \"d3\";\nimport \"./index.css\";\n\n// .tickFormat(5, \"+\");\n\nconst LineChart = props => {\n  const {data, width, height} = props;\n\n  useEffect(() => {\n    drawChart();\n  }, [data]);\n\n  const drawChart = () => {\n    // data processing\n    const allValues = [];\n    const allDates = [];\n    Object.keys(data).forEach(ticker => {\n      allValues.push(data[ticker].map(d => d.roi));\n      allDates.push(data[ticker].map(d => d.date));\n    });\n\n    const padding = {top: 30, right: 30, bottom: 30, left: 30};\n    // for the domain of the x and y axis I will have to look at all values I have\n    // not specific to each stock and build a large array of all those values\n    const x = d3\n      .scaleTime()\n      .domain(d3.extent(allDates.flat(), d => d))\n      .range([0, width - padding.right - padding.left]);\n\n    const y = d3\n      .scaleLinear()\n      .domain([0.8, d3.max(allValues.flat(), d => d)])\n      .range([height - padding.bottom - padding.top, 0]);\n\n    const svg = d3\n      .select(\"#linechart\")\n      .append(\"svg\")\n      .attr(\"width\", width)\n      .attr(\"height\", height)\n      .append(\"g\")\n      .attr(\"transform\", `translate(${padding.left}, ${padding.top})`);\n\n    // call x axis\n    svg\n      .append(\"g\")\n      .attr(\n        \"transform\",\n        `translate(0, ${height - padding.bottom - padding.top})`\n      )\n      .style(\"font\", \"13px times\")\n      .call(d3.axisBottom(x).ticks(5));\n\n    // y axis\n    svg\n      .append(\"g\")\n      .style(\"font\", \"13px times\")\n      .call(d3.axisLeft(y).ticks(5));\n\n    // create color object\n    const colourScheme = {\n      PLTR: \"steelblue\",\n      NIO: \"#004d1f\",\n      SPCE: \"#7d2408\"\n    };\n\n    Object.keys(data).forEach(ticker => {\n      svg\n        .append(\"path\")\n        .datum(data[ticker])\n        .attr(\"fill\", \"none\")\n        .attr(\"stroke\", colourScheme[ticker])\n        .attr(\"stroke-width\", 3)\n        .attr(\n          \"d\",\n          d3\n            .line()\n            .x(d => x(d.date))\n            .y(d => y(d.roi))\n        );\n    });\n  };\n  return <svg id=\"linechart\" height={height} width={width}></svg>;\n};\n\nexport default LineChart;\n","/Users/user/Documents/Projects/fundtracker/client/src/NavBar.js",[],"/Users/user/Documents/Projects/fundtracker/client/src/Dashboard.js",[],"/Users/user/Documents/Projects/fundtracker/client/src/Ranking.js",[],"/Users/user/Documents/Projects/fundtracker/client/src/Ticker.js",[],{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","severity":1,"message":"41","line":17,"column":7,"nodeType":"42","messageId":"43","endLine":17,"endColumn":18},{"ruleId":"44","severity":1,"message":"45","line":12,"column":6,"nodeType":"46","endLine":12,"endColumn":12,"suggestions":"47"},"no-native-reassign",["48"],"no-negated-in-lhs",["49"],"no-unused-vars","'filterDates' is assigned a value but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'drawChart'. Either include it or remove the dependency array.","ArrayExpression",["50"],"no-global-assign","no-unsafe-negation",{"desc":"51","fix":"52"},"Update the dependencies array to be: [data, drawChart]",{"range":"53","text":"54"},[225,231],"[data, drawChart]"]